{
    "$schema": "http://json.schemastore.org/template",
    "author": "MarvelTiter",
    "classifications": [
        "BlazorAdmin",
        "Web"
    ],
    "name": "MtBlazorAdminTemplate",
    "identity": "MtBlazorAdminTemplate",
    "shortName": "mtblazor",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "BlazorAdmin",
    "preferNameDirectory": true,
    "sources": [
        {
            "source": "./",
            "target": "./",
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                ".template.config/**/*",
                "**/*.filelist",
                "**/*.user",
                "*-lock.json",
                "**/TestPages/**",
                "**/.vs/**",
                "**/.idea/**"
            ]
        }
    ],
    "symbols": {
        "ExcludeDefaultService": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "默认页面是否去除默认的服务实现(需要自己实现服务)"
        },
        "ExcludeDefaultPages": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "false",
            "description": "是否去除默认的页面"
        },
        "UseClientProject": {
            "type": "parameter",
            "datatype": "bool",
            "defaultValue": "true",
            "description": "是否使用.Client项目"
        }
    }
}
