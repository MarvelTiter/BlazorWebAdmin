{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSetting": {
    "LauchUrl": "/",
    "LoadUnregisteredPage": true,
    "LoadPageFromDatabase": true,
    "UseAspectProxy": true,
    "AppTitle": "管理网站",
    "AppShortName": "管理网站",
    "LayoutMode": "Card",
    "AppLanguage": "zh-CN",
    "SupportedMajorVersion": 78,
    "OnlineUserPage": {
      "EnableUsers": [ "admin" ],
      "EnableRoles": [ "" ]
    },
    "CameraResolutions": [
      {
        "Name": null,
        "Width": 2560,
        "Height": 1600
      }
    ]
  },
  "Token": {
    "Expire": 15,
    "MaxFreeTime": 900,
    "NeedAuthentication": true
  },
  "CultureOptions": {
    "Enabled": true,
    "SupportedCulture": [
      {
        "Name": "简体中文",
        "Culture": "zh-CN"
      },
      {
        "Name": "English",
        "Culture": "en-US"
      }
    ]
  },
  "ConnectionStrings": {
    "Sqlite": "DataSource=simple.db"
  }
}
