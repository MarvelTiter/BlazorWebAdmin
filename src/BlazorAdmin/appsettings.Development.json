{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSetting": {
    "RunMode": "Server",
    "LauchUrl": "/",
    "LoadUnregisteredPage": true,
    "LoadPageFromDatabase": true,
    "UseAspectProxy": true,
    "AppTitle": "管理网站",
    "AppShortName": "管理网站",
    "LayoutMode": "Card",
    "AppLanguage": "zh-CN",
    "SupportedMajorVersion": 78,
    "ClientHubOptions": {
      "Enable": true,
      "AllowUsers": [
        "admin"
      ],
      "AllowRoles": [
        ""
      ],
      "ServerScanFrequency": "00:05:00",
      "ClientSendFrequency": "00:00:05",
      "ClearTimeoutLimit": "00:00:15"
    },
    "CameraResolutions": [
      {
        "Name": null,
        "Width": 2560,
        "Height": 1600
      }
    ]
  },
  "Token": {
    "Expire": "15.00:00:00",
    "MaxFreeTime": 900,
    "NeedAuthentication": true
  },
  "CultureOptions": {
    "Enabled": true,
    "SupportedCulture": [
      {
        "Name": "简体中文",
        "Culture": "zh-CN"
      },
      {
        "Name": "English",
        "Culture": "en-US"
      }
    ]
  },
  "ConnectionStrings": {
    "Sqlite": "DataSource=simple.db"
  }
}
