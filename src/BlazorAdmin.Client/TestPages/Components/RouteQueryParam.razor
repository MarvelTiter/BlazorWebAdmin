@page "/param/query"
@using Project.Constraints.Common.Attributes
@using Project.Constraints.PageHelper
@implements IRoutePage
@implements IDisposable
@* @attribute [TagTitle("路由参数")] *@
接收的Query路由参数: @Id
@code {
    [SupplyParameterFromQuery] public string? Id { get; set; }
    public string GetTitle()
    {
        return $"详情-{Id}";
    }

    public void OnClose()
    {
        Console.WriteLine($"Id: {Id} -> OnClose");
    }

    public void Dispose()
    {
        Console.WriteLine($"Id: {Id} -> Dispose");
    }

}
