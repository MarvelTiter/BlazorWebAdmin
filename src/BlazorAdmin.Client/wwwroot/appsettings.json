{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSetting": {
    // 是否加载仅由PageGroup和PageInfo标注的页面
    "LoadUnregisteredPage": true,
    // 是否从数据库加载路由配置
    "LoadPageFromDatabase": true,
    // 是否启用切面代理
    "UseAspectProxy": true,
    // 异常页面是否允许显示详细信息
    "ShowExceptionDetial": true,
    "AppTitle": "管理网站",
    "AppShortName": "管理网站",
    // 页面默认布局
    "LayoutMode": "Card",
    // 默认语言
    "AppLanguage": "zh-CN",
    // 开启在线用户页面
    "ClientHubOptions": {
      "Enable": true
    },
    // 自定义摄像头分辨率
    "CameraResolutions": [
      {
        "Name": null,
        "Width": 2560,
        "Height": 1600
      }
    ]
  },
  "CultureOptions": {
    "Enabled": true,
    "SupportedCulture": [
      {
        "Name": "简体中文",
        "Culture": "zh-CN"
      },
      {
        "Name": "English",
        "Culture": "en-US"
      }
    ]
  },
  "ConnectionStrings": {
    "Sqlite": "DataSource=simple.db"
  }
}
